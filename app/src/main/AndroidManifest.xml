<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.twitteralonso">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConfiguracionActivity"></activity>
        <activity android:name=".MandarMensajeActivity" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".EscribirTweetActivity" />
        <activity android:name=".MensajesActivity" />
        <activity android:name=".NotificacionesActivity" />
        <activity android:name=".BuscarActivity" />
        <activity
            android:name=".TimeLineActivity"
            android:label="@string/title_activity_time_line" />
        <activity android:name=".RegistrarseActivity" />
        <activity android:name=".IniSesionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>